local function Crims() 

    _G.InstantR = true
    while _G.InstantR == true do
        wait()
    if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") and
    game.Players.LocalPlayer.Character.Humanoid.Health == 0 then
    if game.Players.LocalPlayer.TeamColor == BrickColor.new("Really red") then
    if workspace:FindFirstChild("tppart") then
    game.Workspace.Name = "Workspace"
    else
    local E = Instance.new("Part")
    E.Anchored = true
    E.Transparency = 1
    E.CanCollide = false
    E.Parent = workspace
    E.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
    E.Name = "tppart"
    
    --if game.Players.LocalPlayer.TeamColor == BrickColor.new("Bright red") then
    
    
    local args = { 
    [1] = "Bright blue"
    
    }
    workspace.Remote.TeamEvent:FireServer(unpack(args))
    
    
    
    
    
    local name = game.Players.LocalPlayer.Name
    
    
    
    wait(0.3)
    game.Workspace:WaitForChild(name)
    
    game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart")
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-920.528137, 95.327179, 2138.6416, -0.985046506, -1.37030307e-08, 0.172288671, -1.4608899e-08, 1, -3.98990174e-09, -0.172288671, -6.44718678e-09, -0.985046506)
    
    
    
    wait(0.3)
    game.Workspace:WaitForChild(name)
    
    game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart")
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.tppart.CFrame
    wait(0.1)
    workspace.tppart:Destroy()
    
    
    
    end
    end
    end
    end
    end
    
    
    
    
    local function Police() 
    
    
    _G.InstantR2 = true
    while _G.InstantR2 == true do
        wait()
    if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") and
    game.Players.LocalPlayer.Character.Humanoid.Health == 0 then
    if game.Players.LocalPlayer.TeamColor == BrickColor.new("Bright blue") then
    
    if workspace:FindFirstChild("tppart") then
    game.Workspace.Name = "Workspace"
    else
    local E = Instance.new("Part")
    E.Anchored = true
    E.Transparency = 1
    E.CanCollide = false
    E.Parent = workspace
    E.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
    E.Name = "tppart"
    
    
    
    
    
    local args = { 
    [1] = "Bright blue"
    
    }
    workspace.Remote.TeamEvent:FireServer(unpack(args))
    local name = game.Players.LocalPlayer.Name
    
    
    
    wait(0.3)
    game.Workspace:WaitForChild(name)
    
    game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart")
    
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.tppart.CFrame
    wait(0.1)
    workspace.tppart:Destroy()
    
    
    
    end
    
    end
    end
    end
    end
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    local function Prisoners() 
    
    _G.InstantR3 = true
    while _G.InstantR3 == true do
        wait()
    if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") and
    game.Players.LocalPlayer.Character.Humanoid.Health == 0 then
    if game.Players.LocalPlayer.TeamColor == BrickColor.new("Bright orange") then
    if workspace:FindFirstChild("tppart") then
    game.Workspace.Name = "Workspace"
    else
    local E = Instance.new("Part")
    E.Anchored = true
    E.Transparency = 1
    E.CanCollide = false
    E.Parent = workspace
    E.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
    E.Name = "tppart"
    
    
    
    
    
    local args = { 
    [1] = "Bright orange"
    
    }
    workspace.Remote.TeamEvent:FireServer(unpack(args))
    local name = game.Players.LocalPlayer.Name
    
    
    
    wait(0.2)
    game.Workspace:WaitForChild(name)
    
    game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart")
    
    
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.tppart.CFrame
    wait(0.1)
    workspace.tppart:Destroy()
    
    
    
    end
    
    end
    end
    end
    end
    
    
    
    
    
    
